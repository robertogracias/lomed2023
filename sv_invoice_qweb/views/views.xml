		<template id="preimpreso">
			<t t-call="web.html_container">
				<t t-foreach="docs" t-as="doc">
					<t t-if="doc.tipo_documento_id.formato=='CCF'" t-call="sv_invoice_qweb.ccf_document" t-lang="doc.partner_id.lang"/>
