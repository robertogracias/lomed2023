	<template id="sv_invoice_qweb.ccfresumen_document" name="sv_invoice_qweb.ccfresumen_document">
			<t t-name="sv_invoice_qweb.ccfresumen_document">
    
    
     <t t-call="web.basic_layout">
      <t t-foreach="docs" t-as="doc">
        <div class="page" name="page">
          
          <t t-set="lineas" t-value="''" />
              <t t-foreach="doc.invoice_line_ids" t-as="item">
                <t t-if="lineas.find(','+str(item.product_id.id))&lt;0">
                  <t t-set="lineas" t-value="lineas+','+str(item.product_id.id)" />
                </t>  
              </t>
              <t t-set="lineas" t-value="lineas[1:]" />

          
          
          
          
          <div style="position:absolute;top:0px;left:0px;font-size: 12px;">
            
            
            
            
            
            
            
            <div style="position:absolute;left:-0px;top:40px;width:810px;height:480px;border:thin solid white">
              
              
              
