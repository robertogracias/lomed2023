                -webkit-transform: rotate(90deg);
                -moz-transform: rotate(90deg);
                -o-transform: rotate(90deg);
                -ms-transform: rotate(90deg);
                transform: rotate(90deg);">
                 <div class="" style="position:absolute;top:12px;left:10px;width:440px;font-size:11px"><span><b>Fecha:</b></span><span t-field="doc.invoice_date"/><span><b>        #Doc:</b></span><span t-field="doc.doc_numero"/></div>
                <div class="" style="position:absolute;top:24px;left:10px;width:440px;font-size:11px"><span><b>Cliente:</b></span><span t-field="doc.partner_id.name"/></div>
                <div class="" style="position:absolute;top:36px;left:10px;width:440px;font-size:11px"><span><b>Dirección:</b></span><span t-field="doc.partner_id.street"/><span>,</span><span t-field="doc.partner_id.city"/></div>
                <div class="" style="position:absolute;top:48px;left:10px;width:440px;font-size:11px"><span><b>NIT:</b></span><span t-field="doc.partner_id.nit"/><span><b>    NRC:</b></span><span t-field="doc.partner_id.nrc"/></div>
                <div class="" style="position:absolute;top:60px;left:10px;width:440px;font-size:11px"><span><b>Giro:</b></span><span t-field="doc.partner_id.giro"/></div>
                
                
                <div style="position:absolute; top:100px;left:5px;">
                  <table class="table o_report_block_table small table-borderless" style="border-bottom:none;width:450px;margin-top:5px;">
                      <thead class="small">
                          <tr>
                              <th></th>
                              <th></th>
                              <th></th>
                              <th></th>
                              <th></th>
                              <th></th>
                          </tr>
                      </thead>
                      <tbody>
                        <t t-foreach="lineas.split(',')" t-as="producto">
                              <t t-set="cantidad" t-value="0"/>
                              <t t-set="nombre" t-value="''"/>
                              <t t-set="precio_subtotal" t-value="0"/>
                              <t t-set="precio_total" t-value="0"/>
                              <t t-foreach="doc.invoice_line_ids" t-as="table_line">
                                <t t-if="producto==str(table_line.product_id.id)">
                                  <t t-set="cantidad" t-value="cantidad+table_line.quantity"/>
                                  <t t-set="nombre" t-value="table_line.display_name"/>
                                  <t t-set="precio_subtotal" t-value="precio_subtotal+table_line.price_subtotal"/>
                                  <t t-set="precio_total" t-value="precio_total+table_line.price_total"/>
                                </t>
                              </t>
                              <tr style="height: 20px;">
                                
                                <td style="width:9%;text-align:right">
                                  <span t-options-widget="&quot;&quot;" style="display:inline-block;font-size: 10px;"><t t-esc="'{0:,.0f}'.format(int(cantidad))" /></span>
                                </td>
                                <td style="width:55%">
                                    <span t-options-widget="&quot;&quot;" style="display:inline-block;font-size: 10px">
                                      <span t-esc="nombre.strip()" />
                                    </span>    
                                </td>
                                <td style="width:9%">
                                    <t t-set="precio" t-value="precio_subtotal/cantidad"/>
                                    <span t-esc="'{0:.2f}'.format(precio)"/>
                                </td>
                                <td style="width:9%">
                                </td>
                                <td style="width:9%">
                                    <span t-esc="'{0:.2f}'.format(precio_total)" t-if="precio_subtotal == precio_total" style=""/>
                                </td>
                                 <td style="width:9%">
                                    <span t-esc="'{0:.2f}'.format(precio_subtotal)" style=""></span>
                                </td>
                              </tr>

                      </t>
                      </tbody>
                  </table>
                </div>
                
                
                <span style="position:absolute;top:540px;left:10px;width:360px;color:black;font-size: 10px;">
